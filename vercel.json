{
  "version": 2,
  "name": "langgraph-trip-planner-agent",
  "functions": {
    "dist/**/*.js": {
      "runtime": "@vercel/node@18.x",
      "maxDuration": 30
    },
    "src/**/*.ts": {
      "runtime": "@vercel/node@18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/agent/(.*)",
      "destination": "/api/agent/$1"
    }
  ]
}
